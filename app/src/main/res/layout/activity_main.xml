<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_search_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search" />

    <Button
        android:id="@+id/btnMoveLogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="login" />

    <Button
        android:id="@+id/btnMoveUserInfo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="UserInfo" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="532dp"
        android:layout_height="513dp"
        android:layout_marginBottom="364dp" />

    <ImageButton
        android:id="@+id/imgbtn_Setting"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="84dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="492dp"
        android:letterSpacing="0.1"
        android:text="TURI"
        android:textColor="#F4E8D9"
        android:textSize="27dp" />

    <ImageButton
        android:id="@+id/imgbtn_User"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="30dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="30dp"
        android:adjustViewBounds="true"
        android:src="@drawable/menu_icon" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="284dp"
        android:layout_height="337dp" />

    <ImageButton
        android:id="@+id/imgbtn_Food"
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/imgbtn_Spot"
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/imgbtn_Stay"
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:layout_marginTop="288dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter" />

    <ImageButton
        android:id="@+id/imgbtn_Enjoy"
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter" />

</LinearLayout>